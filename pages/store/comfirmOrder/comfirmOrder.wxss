}

.settingArea .title-area view:first-child text:last-child {
  font-weight: normal;
  font-size: 24rpx;
  color: #5F5F60;
  /* margin-left: 15rpx; */
}

.settingArea .input-area {
  padding: 20rpx;
  background: #FFF;
}

.settingArea .input-area>view {
  width: 27.5%;
  font-size: 30rpx;
  padding-right: 20rpx;
}

.settingArea .input-area>view:first-child {
  width: 45%;
