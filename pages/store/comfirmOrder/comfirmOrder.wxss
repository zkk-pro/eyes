.help-content {
  background: #fff;
  padding: 0 20rpx 0 20rpx;
}

.label {
  display: inline-block;
  width: 94rpx;
  margin-right: 40rpx;
  text-align: right;
}

.label::after {
  content: ':';
}

.help-item {
  display: flex;
  align-items: center;
  padding-bottom: 20rpx;
}

.help-item input {
  flex: 1;
}

.upload-btn {
  width: 120rpx;
  height: 120rpx;
  border: 1rpx solid #f5f5f5;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #AAA;
}

.upload-btn::before {
  content: '';
  display: block;
  width: 50rpx;
  height: 50rpx;
  background: url('data:image/png;base64,');
  background-size: 100% 100%;
}