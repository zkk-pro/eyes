}

.placeholder-class {
  color: #CBCBCB;
  font-size: 24rpx;
}

.settingArea .input-area view .icon-xiasanjiao {
  font-size: 36rpx;
  color: #909090;
}

.choice-area {
  width: 100%;
  padding: 20rpx;
  background: #FFF;
}

.choice-area .l-area {
  margin-right: 50rpx;
}

.choice-area .iconfont {
  margin-right: 10rpx;
  font-size: 36rpx;
}

.choice-area .icon-choosed2 {
  color: #FD9831;
}

.n-img {
  width: 100rpx;
  height: auto;
}

.b-wrap {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  z-index: 100;
  background: rgba(0, 0, 0, .3);
  padding: 0 10%;
}

.b-list {
  width: 100%;
  padding: 30rpx 20rpx;
  border-radius: 10rpx;
  background: #FFF;
  position: relative;
}

.b-list .b-title {
  width: 100%;
  text-align: center;
  padding: 0rpx 20rpx 20rpx 20rpx;
  color: #FE992E;
  font-size: 30rpx;
  border-bottom: 1px solid #EEE;
}

.b-list .b-info {
  width: 100%;
  padding: 20rpx 0;
  border-bottom: 1px dotted #EEE;
}

.b-list .b-info .icon-choosed2 {
  color: #FD9831;
  font-size: 40rpx;
}

.b-list .b-info .icon-xuanzhongyuandian {
  color: #666;
  font-size: 40rpx;
}

.click-btn {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  color: #FFF;
  background: #FE992E;
  font-size: 28rpx;
  margin-top: 30rpx;
  border-radius: 10rpx;
}

