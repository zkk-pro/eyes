/* pages/order/confirmOrder/confirmOrder.wxss */
page {
  font-family: "微软雅黑";
  background: #F3F6F9;
  font-size: 28rpx;
}

.address-box {
  box-sizing: border-box;
  background: #ffffff;
  padding: 20rpx;
  align-items: center;
}

.address-box .address-left {
  flex: 1;
  box-sizing: border-box;
  padding-right: 30rpx;
}

.address-box .address-left .user-info {
  font-size: 36rpx;
  color: #333333;
  justify-content: space-between;
  padding: 0px 0px 20rpx 0px;
}

.address-box .address-left .address-info {
  line-height: 46rpx;
  color: #999999;
}

.address-box .address-right {
  color: #9F9F9F;
  font-size: 28rpx;
}

.tiao {
  display: block;
  width: 100%;
  height: 10rpx;
}

.red-notice {
  background: #FFF;
  padding: 20rpx;
  color: #EB3230;
  margin-top: 20rpx;
}

/* 商品 */
.goods-list {
  margin-top: 20rpx;
}

.goods-list .goods-item {
  background: #ffffff;
  border-bottom: 1rpx solid #F1F1F1;
  box-sizing: border-box;
  padding: 20rpx;
}

.goods-list .goods-item .goods-img {
  width: 30%;
}

.goods-list .goods-item .goods-img image {
  width: 100%;
}

.goods-list .goods-item .goods-info {
  padding: 0 20rpx;
  box-sizing: border-box;
  flex: 1;
  line-height: 46rpx;
}

.goods-list .goods-item .goods-info .goods-name {
  font-size: 28rpx;
  color: #333333;
  /* line-height: 46rpx; */
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.goods-list .goods-item .goods-info .property {
  font-size: 24rpx;
  color: #999999;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 4;
  -webkit-box-orient: vertical;
}

.goods-list .goods-item .other-info {
  line-height: 46rpx;
  font-size: 32rpx;
  color: #E6303C;
}

.goods-list .goods-item .other-info .count {
  color: #666666;
  text-align: right;
}

.message {
  border-bottom: 1rpx solid #F1F1F1;
  background: #ffffff;
  color: #333333;
  padding: 20rpx;
  font-size: 26rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-top: 10rpx;
}

.message input {
  flex: 1;
  height: 70rpx;
  padding-left: 40rpx;
  font-size: 28rpx;
}

.server {
  border-bottom: 1rpx solid #F1F1F1;
  background: #ffffff;
  color: #333333;
  padding: 20rpx;
  font-size: 28rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-top: 10rpx;
}

.server input {
  width: 300rpx;
  height: 50rpx;
  text-align: right;
  font-size: 28rpx;
}

.person-info {
  padding: 0rpx 20rpx;
  background: #fff;
  margin-top: 10rpx;
}

.person-info .personInfo {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 15rpx 0rpx;
  border-bottom: 1rpx solid #eee;
}

.person-info .personInfo input {
  flex: 1;
  text-align: right;
  height: 70rpx;
  font-size: 28rpx;
}

.delivery-area {
  padding: 0px 20rpx;
  background: #fff;
  margin-top: 10rpx;
}

.delivery-area .express {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 0rpx;
}

.express.exp-type {
  border-bottom: 1rpx solid #eee;
}

.express .icon-you2 {
  color: #999999;
  font-size: 26rpx;
}

.express .icon-iconfontxuanzhong_1 {
  color: #F0CCAC;
  font-size: 30rpx;
}

.express .icon-xuanzhongyuandian {
  color: #999;
  font-size: 30rpx;
}

.total-wrap {
  border-bottom: 1rpx solid #F1F1F1;
  background: #ffffff;
  color: #333333;
  padding: 30rpx 20rpx;
  font-size: 28rpx;
  text-align: right;
  margin-bottom: 140rpx;
}

.total-wrap .goods-count {
  padding-right: 40rpx;
  font-size: 28rpx;
}

.total-wrap .total-price {
  font-size: 28rpx;
}

.total-wrap .total-price text {
  font-size: 34rpx;
}

.coupen-wrap {
  border-bottom: 1rpx solid #F1F1F1;
  border-top: 1rpx solid #F1F1F1;
  background: #ffffff;
  color: #333333;
  padding: 30rpx 20rpx;
  justify-content: space-between;
  margin-top: 10rpx;
  /* margin-bottom: 10rpx; */
}

.coupen-wrap .icon-you2 {
  color: #999999;
  font-size: 26rpx;
}

.coupen-list {
  position: fixed;
  z-index: 100;
  bottom: 0;
  width: 100%;
  height: 400rpx;
  border-top: 1rpx solid #ececec;
  border-bottom: 1rpx solid #ececec;
  text-align: center;
  line-height: 50px;
  background: #ffffff;
}

.right-arrow {
  width: 40rpx;
  height: 30rpx;
}

/*验光数据*/
.user-info {
  padding: 0rpx 20rpx;
  font-size: 32rpx;
  margin-top: 10rpx;
  padding-right: 40rpx;
  background: #fff;
}

.info-top {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 20rpx 0rpx;
}

.eye-item {
  display: flex;
  flex: 1;
}

.info-right {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  color: #999;
}

.info-left {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  color: #999;
}

.degree-area {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 20rpx 0rpx;
}

.right-area {
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 2rpx solid #EEE;
  padding-bottom: 10rpx;
  text-align: right;
  margin-left: 30rpx;
  position: relative;
}

.input-area {
  font-size: 26rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
  flex: 1;
  text-align: center;
}

.iconfont.icon-xiasanjiao1 {
  font-size: 30rpx;
  color: #999;
  position: relative;
  top: -6rpx;
}

.left-area {
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 2rpx solid #EEE;
  padding-bottom: 10rpx;
  text-align: right;
  margin-left: 30rpx;
}

.degree-area picker {
  width: 40rpx;
  /* position:absolute;
	width:100%;
	height:100%;
	top:0px;
	left:0px;
	background:rgba(0,0,0,0);
	text-align:right; */
}

/* 底部 */
.bottom-wrap {
  position: fixed;
  bottom: 0;
  z-index: 40;
  height: 100rpx;
  box-sizing: border-box;
  width: 100%;
}

.bottom-wrap .bottom-right {
  height: inherit;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #ffffff;
  text-align: center;
  padding: 0 50rpx;
  background: #FE8A22;
  border-top: 1px solid #FE8A22;
}

.bottom-wrap .bottom-left {
  display: flex;
  flex: 1;
  box-sizing: border-box;
  border-top: 1rpx solid #ececec;
  background: #ffffff;
  align-items: center;
  justify-content: flex-start;
  font-size: 24rpx;
  color: #666666;
  padding-left: 30rpx;
}

.bottom-wrap .bottom-left .total-price {
  font-size: 28rpx;
  margin-left: 40rpx;
}

.bottom-wrap .bottom-left .total-price text {
  color: #FE5F1C;
}

/* 弹框 */
.tip-wrap {
  position: fixed;
  margin: auto;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, .5);
  z-index: 500;
}

.tip-wrap .tip-cont {
  position: fixed;
  margin: auto;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: #ffffff;
  width: 90%;
  height: 400rpx;
  border-radius: 12rpx;
}

.tip-wrap .tip-cont .tip-top {
  text-align: right;
  box-sizing: border-box;
  padding: 20rpx;
}

.tip-wrap .tip-cont .off-icon {
  width: 40rpx;
}

.tip-wrap .tip-cont .cont-detail {
  margin: 20rpx 0;
  font-size: 36rpx;
  color: #333333;
  text-align: center;
}

.tip-wrap .tip-cont .btn-wrap {
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 80rpx;
}

.tip-wrap .tip-cont .btn-wrap .btn-item {
  width: 160rpx;
  padding: 10rpx 0;
  color: #ffffff;
  font-size: 32rpx;
  border-radius: 8rpx;
  text-align: center;
  margin: 0 20rpx;
}

.tip-wrap .tip-cont .btn-wrap .btn-item.cancel {
  background: #FF7F00;
}

.tip-wrap .tip-cont .btn-wrap .btn-item.sure {
  background: #EA6360;
}

.no-address {
  height: 120rpx;
  display: flex;
  flex: 1;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.no-address text {

  width: 260rpx;
  height: 64rpx;
  line-height: 60rpx;
  border-radius: 64rpx;
  color: #FE9032;
  font-size: 30rpx;
  border: 2rpx solid #FE9032;
  text-align: center;
}

.icon-weixinzhifu {
  color: #00B600;
}

.icon-yue {
  color: #FE9F33;
}

.settingArea .title-area {
  padding: 20rpx 20rpx 20rpx 20rpx;
}

