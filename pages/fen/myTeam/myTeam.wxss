.top-nav {
    width: 100%;
    background: linear-gradient(to right,#FCB92A,#FED363);
    padding: 30rpx;
    color: #FFF;
}

.user-info .user image{
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    margin-right: 30rpx;
}
.user-info .user > view text:first-child{
    font-size: 32rpx;
    margin-bottom: 10rpx;
}
.user-info .user > view text:last-child{
    font-size: 24rpx;
}
.user-info .team-num{
    text-align: center;
    font-size: 24rpx;
}
.user-info .team-num text:first-child{
    font-size: 32rpx;
    margin-bottom: 10rpx;
}
.top-nav  .fen-desc{
    font-size: 22rpx;
    margin-top: 20rpx;
}
.title-tab{
    width: 100%;
    padding: 20rpx;
    background: #FFF;
    font-size: 24rpx;
    border-bottom: 1px solid #F9F9F9;
}
.title-tab:nth-child(2){
    margin-bottom: 10rpx;
    font-size: 28rpx;
}
.title-tab view{
    width: calc( 70% / 4 );
    text-align: center;
}
.title-tab view:nth-child(2){
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;

}
.title-tab view:nth-child(3){
    width: 30%;
    color: #A1A1A1;
    font-size: 20rpx;
}
.title-tab view:nth-child(4){
    color: #A1A1A1;

}
.title-tab:nth-child(2) view:nth-child(3){
    color: #333;
    font-size: 28rpx;
}
.title-tab:nth-child(2) view:nth-child(4){
    color: #333;
}
.title-tab view image{
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}
.title-tab view .icon-bianji{
    color:#FFB83B;
    font-size: 24rpx;
    margin-left: 10rpx;
}
.rate-wrap{
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    background: rgba(0,0,0,0.3);
}
.rate-wrap .rate{
    width: 80%;
    background: #FFF;
    margin-left: 10%;
    border-radius: 10rpx;
    padding: 30rpx 0;
    position: relative;
}
.rate-wrap .rate .title{
    width:100%;
    text-align: center;
    padding: 40rpx 0 40rpx 0;

}
.rate-wrap .rate .title .icon-no{
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    color: #999;
    font-size: 38rpx;
}
.rate-wrap .rate .rate-input{
    width: 100%;
    padding: 0 20%;
}
.rate-wrap .rate .rate-input input{
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    border: 1px solid #EEE;
    border-radius: 6rpx;
    padding:0 20rpx;
    text-align: center;
}
.rate-wrap .rate .btn-click{
    width:60%;
    padding: 15rpx;
    background: linear-gradient(to right,#FCB92A,#FED363);
    color: #FFF;
    margin: 40rpx 20%;
    text-align: center;
    font-size: 28rpx;
    border-radius: 6rpx;
}