page{
  padding-bottom: 80rpx;
}
.wrap .header-img{
  width: 100%;
  background: #FFF;
  position: relative;
}
.wrap .header-img > image{
  width: 100%;
  height: auto;
}
.wrap .header-img .title{
  position: fixed;
  top: 4.6%;
  width: 100%;
  font-size: 29rpx;
  text-align: center;
  z-index: 99999999;
}
.parentUser{
  witdh:100%;
  position: absolute;
  left: 0;
  top:26%;
  color: #FFF;
  z-index: 999;
  padding: 0 8%;
}
.parentUser image{
  width: 120rpx;
  height: 120rpx;
  border-radius:50%;
  border: 6rpx solid #FFE180;
  margin-right: 20rpx;
}
.parentUser .right{
  flex: 1;
}
.parentUser .right view:last-child{
  margin-top: 10rpx;
}
.search-area{
  width: 100%;
  position: absolute;
  top: 53%;
}
.search-area .search{
  width: 84%;
  position: absolute;
  top: 0;
  left: 8%;
  z-index: 999;
  padding: 10rpx 20rpx;
  border-radius: 20rpx;
  background: rgba(255,255,255, 0.6);
}
.search-area .search .icon-sousuo{
  margin-right: 10rpx;
  color: #855908;
  font-size: 36rpx;
}
.swiper{
  width: 100%;
  margin-top: -20%;
  background: #FFF;
  padding-bottom: 20rpx;
}
.swiper swiper{
  width: 100%;
  height: 350rpx;

}
.swiper swiper swiper-item{
  width: 100%;
}
.swiper swiper swiper-item .out{
  width: 100%;
}
.swiper swiper swiper-item .in{
  transform: scale(0.9);
}
.swiper swiper swiper-item  .selected{
  transform: scale(1);
}
.swiper swiper image{
  width: 100%;
  height: 350rpx;
  border-radius: 20rpx;
}
.category-list{
  width: 100%;
  padding:20rpx 7%;
  background: #fff;
}
.category-list .category{
  width: 20%;
  margin-bottom: 20rpx;
}
.category-list .category .img{
  width:100%;
  margin-bottom: 10rpx;
}
.category-list .category .img image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.ad{
  width: 100%;
  background: #FFF;
  padding: 20rpx 7%;
 
}
.ad image{
  width: 100%;
  height: auto;
  position: relative;
  z-index: 100;
}
.simple-desc{
    width: 100%;
    background: #FFF;
    padding: 30rpx;
    margin-top: 20rpx;
}
.simple-desc image{
  width: 130rpx;
  height: auto;
}
.simple-desc view text{
  color: #9D9D9D;
}
.simple-desc view text:last-child{
  font-size: 24rpx;
  margin-left: 10rpx;
  margin-top: 5rpx;
}
scroll-view{
  width: 100%;
  background: #FFF;
  padding: 15rpx 15rpx 30rpx 15rpx;
  white-space: nowrap;
}

scroll-view .goods{
  display: inline-block;
  width: 40%;
  margin:20rpx 15rpx;
  padding: 20rpx;
  border-radius: 10rpx;
  box-shadow: 0px -3px 3px 0px rgba(200,200,200,0.2), 
              -3px 0px 3px 0px rgba(200,200,200,0.2), 
              3px 0px 3px 0px rgba(200,200,200,0.2), 
              0px 3px 3px 0px rgba(200,200,200,0.2);
}
scroll-view .goods image{
  width: 100%;
  height: auto;
  border:1px solid #EEE;
  border-radius: 4rpx;
}
scroll-view .goods .goods_name{
  width: 100%;
  font-size: 28rpx;
	overflow: hidden;
	text-overflow: ellipsis;
  padding-top: 15rpx;
}
scroll-view .goods .price{
  width: 100%;
}
scroll-view .goods .price{
  padding-top: 15rpx;
}
scroll-view .goods .price > text:first-child{
  color: #9A9A9A;
  text-decoration: line-through;
  font-size: 22rpx;
}
scroll-view .goods .price > text:last-child{
  width: 80rpx;
  height: 35rpx;
  font-size: 22rpx;
  text-align: center;
  line-height: 35rpx;
  border-radius: 35rpx;
  background: #FE6A57;
  color: #FFF;
}
scroll-view .goods .price view:first-child text:first-child{
   font-size: 24rpx;
   color:#FD6443;
}
scroll-view .goods .price view:first-child text:last-child{
  font-size: 18rpx;
  color:#9A9A9A;
  text-decoration: line-through;
}
scroll-view .goods .price > view:last-child{
    width: 120rpx;
    height: 35rpx;
    font-size: 22rpx;
    text-align: center;
    line-height: 35rpx;
    border-radius: 35rpx;
    background: #FE811B;
    color: #FFF;  
}
scroll-view::-webkit-scrollbar {
  width:0;  
  height:0;  
  color:transparent;
}
.store-desc{
  width: 100%;
  background: #FFF;
  padding: 30rpx;
  margin-top: 20rpx;
}
.store-desc .s-left text{
  font-size: 32rpx;
  font-weight: 700;
}
.store-desc .s-left image{
  width: 170rpx;
  height: auto;
  margin-left: 20rpx;
}
.store-desc .s-right text:first-child{
  color: #9A9A9A;
}
.store-desc .s-right text:last-child{
  color: #9A9A9A;
  margin-left: 10rpx;
  font-size: 24rpx;
  margin-top: 5rpx;
}
.store-goods-list{
  width:100%;
  padding: 10rpx 30rpx 30rpx 30rpx;
  background: #FFF;
}
.store-goods-list .store-goods{
  width: 100%;
  padding: 30rpx;
  border-radius: 10rpx;
  box-shadow: 0px -3px 3px 0px rgba(200,200,200,0.2), 
              -3px 0px 3px 0px rgba(200,200,200,0.2), 
              3px 0px 3px 0px rgba(200,200,200,0.2), 
              0px 3px 3px 0px rgba(200,200,200,0.2);
  margin-top: 30rpx;
}
.store-goods-list .store-goods:first-child{
  margin-top: 0rpx;
}
.store-goods-list .store-goods .g-left .g-left-image{
   width: 180rpx;
   height: auto;
   border:1px solid #EEE;
   border-radius: 4rpx;
   margin-right: 20rpx;
   border-radius: 10rpx;
}
.store-goods-list .store-goods .g-left {
  position: relative;
}
.store-goods-list .store-goods .g-left .store-goods-sort {
  position: absolute;
  left: 0;
  top: 0;
}
.store-goods-list .store-goods .g-left .store-goods-sortImg {
  width: 68rpx;
  border: none;
  border-top-left-radius: 10rpx;
}
/* .store-goods-list .store-goods .g-left .store-goods-sortNum {
  position: absolute;
  z-index: 10;
  color: #fff;
  font-size: 24rpx;
} */
.store-goods-list .store-goods .g-right{
  flex: 1;
}
.store-goods-list .store-goods .g-right .g-title{
  width: 100%;
  font-size: 30rpx;
  text-overflow: -o-ellipsis-lastline;
	overflow: hidden;
	text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
}
.store-goods-list .store-goods .g-right .g-info{
  margin-bottom: 6rpx;
}
.store-goods-list .store-goods .g-right .g-info .i-l text:first-child{
  color: #FD4F27;
  margin-right: 10rpx;
  font-size: 26rpx;
}
.store-goods-list .store-goods .g-right .g-info .i-l text:nth-child(2){
  color: #9A9A9A;
  text-decoration: line-through;
  font-size: 22rpx;
}
.store-goods-list .store-goods .g-right .g-info .i-l text.commission{
  width: 120rpx;
  height: 30rpx;
  line-height: 30rpx;
  border-radius: 5rpx;
  color: #D8B778;
  background: #373636;
  font-size: 18rpx;
  text-align: center;
  margin-left: 10rpx;
}
.store-goods-list .store-goods .g-right .g-info .buy-btn{
   width: 130rpx;
   height: 45rpx;
   text-align: center;
   line-height: 45rpx;
   border-radius: 45rpx;
   color: #FFF;
   background: linear-gradient(to right,#FFA300,#FEBF2A);
   margin-left: 10rpx;
   font-size: 24rpx;
   transition: all 0.5s;
}
.buy-btn:active{
  zoom: 0.98;
  /* background: linear-gradient(to right,#694a14,#583f05); */
}
.padding-bottom{
  padding: 60rpx;
  background: #FFF;
}
.ad-img{
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    background: rgba(0,0,0,0.2);
    padding:0 13%;
    z-index: 1999;
}
.ad-img .img-wrap{
    width: 100%;
    padding: 15rpx;
    background: #FFF;
    border-radius: 10rpx;
}
.ad-img .img-wrap image{
    display: block;
    width: 100%;
    height: auto;
    position: relative;
}
.ad-img .img-wrap image .second{
    display: inline-block;
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    padding: 0 15rpx;
    height: 35rpx;
    line-height: 35rpx;
    border-radius: 35rpx;
    background:#6B6B6C;
    color: #FFF;
    font-size: 22rpx;
}
.ad-img .iconfont{
    color: #2B2D30;
    margin-top: 80rpx;
    font-size: 45rpx;
}
.acti-wrap{
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    background: rgba(0,0,0,0.3);
    padding:0 10%;
    z-index: 2000;
}
.acti-wrap .acti-img{
    width: 100%;
}
.acti-wrap .acti-img image{
    width: 100%;
    height: auto;
}
.acti-wrap .cancel-icon{
    width: 100%;
    text-align: center;
    margin-top: 80rpx;
}
.acti-wrap .cancel-icon .iconfont{
    font-size: 42rpx;
    color: #1F272F;
}
.test-wrap{
  width: 100%;
  position: relative;
  z-index: 100;
}